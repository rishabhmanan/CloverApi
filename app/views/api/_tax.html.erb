<html>
  <body>
    <h2>Tax Rates:</h2>
    <table>
      <thead>
        <tr>
          <th>Tax ID</th>
          <th>Tax Name</th>
          <th>Tax Rate</th>
        </tr>
      </thead>
      <tbody>
        <% @total_taxes.each do |tax|%>
          <tr>
            <td><%= tax["id"] %></td>
            <td><%= tax["name"] %></td>
            <td><%= tax["rate"] %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </body>
</html>